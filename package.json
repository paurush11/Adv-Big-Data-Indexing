  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch": "tsc -w",
    "start": "ts-node src/index.ts",
    "start2": "node dist/index.js",
    "prettier": "prettier --write 'src/**/*.{js,ts,tsx,jsx}'",
    "dev": "prettier --write 'src/**/*.{js,ts,tsx,jsx}' && nodemon dist/index.js",
